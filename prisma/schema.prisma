datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model foodtruck {
  id            Int      @id @default(autoincrement())
  name          String
  category      String
  description   String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
  
  @@map("foodtrucks")
}
model foodItem {
  id            Int      @id @default(autoincrement())
  name          String
  category      String
  quantity      Int
  expiry        String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
  
  @@map("foodItems")
}
model inventory {
  id            Int      @id @default(autoincrement())
  name          String
  description   String
  quantity      Int
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
  
  @@map("inventorys")
}
